#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/dist/bin/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/dist/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/dist/bin/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/dist/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/next/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/next@16.1.3_@babel+core@7.28.6_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/Users/tway/Projects/monorepo-learn/saaas-product/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
